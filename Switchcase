package week1;

public class Switch {
public static void main(String[] args) {
	int month=11;	
	switch(month)
	{
	case 1:
		System.out.println ("Jan");
		break;
	case 2:
		System.out.println ("Feb");
		break;		
	case 3 :
		System.out.println("Mar");
		break;
		
	default:
			System.out.println ("No months available");
		
}

}
}
